<%- include('../includes/header')-%>
<section class="container margin-top top">
  <form action="/auth/login" method="post" class="form">
    <input type="hidden" name="_csrf" value="<%= csrfToken %>" />
    <div class="flex-center width-full padding-md">
      <div class="flex-1">
        <h1>Hello, Welcome!</h1>
        <% if(errorMsg){%>
        <p class="bg-red width-full"><%=errorMsg%></p>
        <%}%>
        <div class="">
          <label for="email">Email</label
          ><input
            class="width-80"
            type="text"
            name="email"
            id="email"
            required
          />
        </div>
        <div class="">
          <label for="password">Password</label
          ><input
            class="width-80"
            type="password"
            name="password"
            id="password"
            required
          />
        </div>
        <button class="btn">Login</button>
        <a href="/auth/reset-password" class="red btn margin-top"
          >Forget Your Password</a
        >
      </div>
      <div class="flex-1">
        <img
          class="cover"
          src="data:image/jpeg;base64,"
          alt=""
        />
      </div>
    </div>
  </form>
</section>
<%- include('../includes/footer')-%>
